@model List<AppRole>

    <a asp-action="Create" asp-controller="Role">Rol ekle</a>


    <table class="table table-hover">
        <thead>
            <tr>
                <th>ID</th>
                 <th>Rol ismi</th>
            </tr>
        </thead>

        <tbody>
            @foreach (AppRole item in Model)
        {
            <tr>
                <td>@item.Id</td>
                <td>@item.Name</td>
            </tr>
        }
        </tbody>
    </table>